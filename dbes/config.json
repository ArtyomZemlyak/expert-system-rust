    "tracker_parser":{
        "fields_find": {
            "title":    0,
            "h1":       {"priority": 3, "tags_from_text": true},
            "h2":       {"priority": 2, "tags_from_text": true},
            "h3":       {"priority": 2, "tags_from_text": true},
            "h4":       {"priority": 2, "tags_from_text": true},
            "h5":       {"priority": 2, "tags_from_text": true},
            "h6":       {"priority": 2, "tags_from_text": true},
            "strong":   {"priority": 2, "tags_from_text": true},
            "body":     {"priority": 2, "tags_from_text": true},
            "a":        {"priority": 2, "tags_from_text": true},
            "p":        {"priority": 2, "tags_from_text": true},
            "li":       {"priority": 2, "tags_from_text": true}
        }
    },

    "html_parser": {
        "fields_find": {
            "title":    {"priority": 10, "tags_from_text": true},
            "h1":       5,
            "h2":       4,
            "h3":       3,
            "h4":       2,
            "h5":       2,
            "h6":       2,
            "strong":   3,
            "body":     1,
            "a":        5,
            "p":        1,
            "li":       1
        }
    },

    "html_gosu_parser": {
        "fields_find": {
            "title":                                  {"priority": 10, "tags_from_text": true},
            "ng-include[@class=\"ng-scope\"]":        1,
            "div[@data-compile-html=\"modal.text\"]": 1
        }
    },

    "finder": {
        "count_of_tags_for_display":    100,
        "count_of_res_for_display":     25,
        "valid_chars": "йцукенгшщзхъфывапролджэячсмитьбюёЙЦУКЕНГШЩЗХЪФЫВАПРОЛДЖЭЯЧСМИТЬБЮЁ",
        "stop_words": ["мфц", "bi", "digit", "confluence", "в", "для", "и", "по", "с", "на", "о", "из", "до", "к", "у", "за", "г."]
    },

    "visualizer": {
        "max_filter_of_count": 30000000
    },

    "ml_learner": {
        "data": {
            "train": 0.9,
            "val":   0.0,
            "test":  0.1,
            "balanced": false
        },
        "pos_tags": false,
        "tags_max_norm": true,
        "tag_threshold": 0.1,
        "rare_tag": true,
        "threshold_up":     0.9,
        "threshold_bottom": 0.1,
        "min_counter": 0,
        "dont_know_class": false,
